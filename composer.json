{
    "name": "jlevers/selling-partner-api",
    "version": "2.2.1",
    "description": "PHP client for Amazon's Selling Partner API",
    "keywords": [
        "api",
        "selling-partner-api",
        "amazon",
        "ecommerce",
        "sp-api",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Jesse Evers",
            "homepage": "https://jesseevers.com"
        }
